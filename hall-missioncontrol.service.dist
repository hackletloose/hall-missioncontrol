[Unit]
Description=missioncontrol - Tool for Managing System Services in Discord
After=network.target

[Service]
User=<your-username>
WorkingDirectory=/path/to/your/hall-missioncontrol
ExecStart=/usr/bin/python3 /path/to/your/hall-missioncontrol/missioncontrol.py
Restart=always

[Install]
WantedBy=multi-user.target
